{
  "appName": "MyFlutterApp",
  "version": "1.0.0",
  "description": "A sample Flutter application.",
  "flutter": {
    "minSdkVersion": 21,
    "targetSdkVersion": 33,
    "compileSdkVersion": 33,
    "dartVersion": "2.19.0"
  },
  "dependencies": {
    "flutter": "sdk: flutter",
    "provider": "^6.1.3",
    "http": "^0.15.0",
    "shared_preferences": "^2.1.0"
  },
  "dev_dependencies": {
    "flutter_test": "sdk: flutter",
    "mockito": "^5.0.16"
  },
  "assets": [
    "assets/images/logo.png",
    "assets/icons/icon.png"
  ],
  "screens": {
    "home": {
      "title": "Home",
      "route": "/",
      "widget": "HomeScreen"
    },
    "settings": {
      "title": "Settings",
      "route": "/settings",
      "widget": "SettingsScreen"
    }
  },
  "build": {
    "release": {
      "buildNumber": "1",
      "buildName": "1.0.0"
    },
    "debug": {
      "buildNumber": "1",
      "buildName": "1.0.0"
    }
  },
  "ios": {
    "deploymentTarget": "13.0",
    "bundleIdentifier": "com.example.myflutterapp"
  },
  "android": {
    "applicationId": "com.example.myflutterapp",
    "minSdkVersion": 21,
    "targetSdkVersion": 33,
    "compileSdkVersion": 33
  }
}
